
public class Main {

	public static void main(String[] args) {
		int monthNumber = 3;
		
		switch(monthNumber) {
		case 1:
			System.out.println("Its January!");
			break;
		case 2:
			System.out.println("It's Feb");
			break;
		case (>12):
			System.out.println("Noob, its 1-12");
			break;
		default:
			System.out.println("It ain't Jan or Feb sweetie");
			break;
		}

	}

}
