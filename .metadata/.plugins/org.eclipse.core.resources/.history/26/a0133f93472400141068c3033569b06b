
public class Main {

	public static void main(String[] args) {
		int monthNumber = 1;
		
		switch(monthNumber) {
		case 1:
			System.out.println("Its January!");
			break;
		case 2:
			System.out.println("It's Feb");
			break;
		default:
			System.out.println("It ain't Jan or Feb sweetie");
			break;
		}

	}

}
